{
  "wait": {
    "description": "Ожидает указанное количество времени или выполнение условия",
    "syntax": "wait(время|условие)",
    "examples": [
      "wait(5)", 
      "wait(условие)"
    ]
  },
  "start": {
    "description": "Запускает выполнение процесса или действия",
    "syntax": "start(процесс)",
    "examples": [
      "start(процесс1)"
    ]
  },
  "stop": {
    "description": "Останавливает выполнение процесса или действия",
    "syntax": "stop(процесс)",
    "examples": [
      "stop(процесс1)"
    ]
  }
}